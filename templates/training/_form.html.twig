{{ form_start(form) }}


    <div class="row">

        <div class="col-lg-6 col-sm-12">
            {% if edit == 1 %}
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">{{ form_row(form.trainingAt) }}</div>
            </div>
            {% endif %}
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12">{{ form_row(form.category) }}</div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-sm-12 col-md-12">{{ form_row(form.startedAt) }}</div>
                <div class="col-lg-6 col-sm-12 col-md-12">{{ form_row(form.endedAt) }}</div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-sm-12 col-md-12">{{ form_row(form.trainingDay) }}</div>
                <div class="col-lg-6 col-sm-12 col-md-12">{{ form_row(form.trainingHour) }}</div>
            </div>
        </div>

    </div>



<div class="row">
    <div class="col-lg-12 col-sm-12 col-md-12">{{ form_row(form.submit) }}</div>
</div>

{{ form_end(form) }}